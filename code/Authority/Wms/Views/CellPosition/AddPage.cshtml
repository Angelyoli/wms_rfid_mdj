@{
    Layout = null;
}
<div class="easyui-layout" fit="true" border="false">
	<div region="center" border="false" style="padding:10px 10px;background:#fff;border:1px solid #ccc; " > 
      <form id="fm" method="post" >  
        <table width="280">
                    <tr>
                    <td width="75" align="right">货位位置ID：</td>
                    <td width="168">
                    <input name="ID" id="inputID" type="text" readonly="readonly" value="自动增长"/>
                    <input name="ID"  style="display:none" /></td>
                    </tr>

                    <tr>
                    <td width="75" align="right">货位代码：</td>
                    <td width="168">
                    <input name="CellCode" id="inputCellCode" class="easyui-validatebox" required="true" type="text"/>
                    <input name="ID"  style="display:none" /></td>    
                    </tr>

                    <tr>
                    <td width="75" align="right">入库位置：</td>
                    <td width="168">
                        <input name="StockInPositionID" type="text" id="InPositionId" style="display: none;" />
                        <input name="StockInPosition" id="inputStockInPosition" type="text"/>
                        <a href="#" id="StockInPositionSearch" class="easyui-linkbutton" iconcls="icon-search"
                            plain="true" onclick="StockInPositionSearchClick()"></a>
                    </td>    
                    </tr>

                    <tr>
                    <td width="75" align="right">出库位置：</td>
                    <td width="168">
                        <input name="StockOutPositionID" type="text" id="OutPositionId" style="display: none;" />
                        <input name="StockOutPosition" id="inputStockOutPosition" type="text"/>
                        <a href="#" id="StockOutPositionSearch" class="easyui-linkbutton" iconcls="icon-search"
                            plain="true" onclick="StockOutPositionSearchClick()"></a>  
                    </tr>

                </table>
            </form>
    </div>
</div>
@Html.Partial("_ParentPositionNamePartial")
