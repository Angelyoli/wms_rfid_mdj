@{
    Layout = null;
}
<div class="easyui-layout" fit="true" border="false">
	<div region="center" border="false" style="padding:10px 10px;background:#fff;border:1px solid #ccc; " > 
      <form id="fm" method="post" >  
       <table width="280" border="0" cellspacing="7" cellpadding="7">
        <tr style="display: none;">
                        <td  width="75">货位ID</td>
                        <td width="168">
                        <input name="ID" id="inputID" class="easyui-validatebox" type="text" />
                        </td>
                    </tr>

                    <tr>
                    <td width="75" align="right">货位名称：</td>
                    <td width="168">
                    <input name="CellCode" readonly="true" id="CellCode1" style="display: none;"  />
                    <input name="CellName" id="inputCellName" class="easyui-validatebox" required="true" readonly="readonly" type="text"/>
                    <a href="#" id="CellSelect" onclick="CellSelectClick()" class="easyui-linkbutton" iconcls="icon-search" plain="true">
                    </td>
                    </tr>
                    <tr>
                    <td width="75" align="right">入库位置：</td>
                    <td width="168">
                        <input name="StockInPositionID" type="text" id="StockInPositionID" style="display: none;" />
                        <input name="InName" id="inputInName" class="easyui-validatebox" required="true" readonly="readonly" type="text"/>
                        <a href="#" id="StockInPositionSearch" class="easyui-linkbutton" iconcls="icon-search"
                            plain="true" onclick="CellPositionSearchClick(3)"></a>
                    </td>    
                    </tr>

                    <tr>
                    <td width="75" align="right">出库位置：</td>
                    <td width="168">
                        <input name="StockOutPositionID" type="text" id="StockOutPositionID" style="display: none;" />
                        <input name="OutName" id="inputOutName" class="easyui-validatebox" required="true" readonly="readonly" type="text"/>
                        <a href="#" id="StockOutPositionSearch" class="easyui-linkbutton" iconcls="icon-search"
                            plain="true" onclick="CellPositionSearchClick(4)"></a>  
                    </tr>

                </table>
            </form>
    </div>
</div>
@Html.Partial("_CellPositionNamePartial")
@*@Html.Partial("_CellCodePartial")*@
@Html.Partial("_CellNamePartial")

