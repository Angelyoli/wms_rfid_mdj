@{
    Layout = null;
}
<div class="easyui-layout" fit="true" border="false">
	<div region="center" border="false" style="padding:10px 10px;background:#fff;border:1px solid #ccc; " > 
      <form id="fm" method="post" >  
        <table width="280">
                    <tr>
                    <td width="60" align="right">ID：</td>
                    <td width="168">
                    <input name="ID" id="inputID"  required="true" type="text" readonly="readonly" value="自动增长"/>
                    <input name="ID"  style="display:none" /></td>

                    <tr>
                    <td width="60" align="right">路径ID：</td>
                    <td width="168">
                    <input name="PathID" id="inputPathID" class="easyui-validatebox" required="true" type="text"/>
                    <input name="PathID"  style="display:none" /></td>
                        
                    </tr>

                    <tr>
                    <td width="60" align="right">位置ID：</td>
                    <td width="168">
                    <input name="PathID" id="inputPositionID" class="easyui-validatebox" required="true" type="text"/>
                    <input name="PathID"  style="display:none" /></td>

                    </tr> 
                                         
                    <tr>
                    <td width="60" align="right">路径节点顺序：</td>
                    <td width="168">
                    <input name="PathID" id="inputPathNodeOrder" class="easyui-validatebox" required="true" type="text"/>
                    <input name="PathID"  style="display:none" /></td>
	                
                    </tr>
                </table>
            </form>
    </div>
</div>
