@{
    Layout = null;
}
<div class="easyui-layout" fit="true" border="false">
	<div region="center" border="false" style="padding:10px 10px;background:#fff;border:1px solid #ccc; " >   
        <table width="300">
                    <tr>
                    <td width="60">路径ID:</td>
                    <td width="168"><input id="ID" type="text"/>
                    <input name="ID"  style="display:none" /></td>
                    </tr>
                    <tr>
                    <td width="60">路径名称：</td>
                    <td width="168"><input id="PathName" type="text"/></td>
                    </tr> 
                    <tr>
                    <td width="60">起始区域ID：</td>
                    <td width="168"><input id="OriginRegionID" type="text"/></td>
                    </tr>  
                    <tr>
                    <td width="60">目标区域ID：</td>
                    <td width="168"><input id="TargetRegionID" type="text"/></td>
                    </tr>                       
                    <tr>
                    <td width="60">状态：</td>
                    <td><select id="state" class="easyui-combobox" required="true" style="width: 130px">
                            <option value="">请选择</option>
		                    <option value="1">可用</option>
		                    <option value="0">不可用</option>
	                    </select>
	                </td>
                    </tr>
                </table>
    </div>
</div>



