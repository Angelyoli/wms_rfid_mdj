<script type="text/javascript">
    function printClick() {
        var billTypeCode = null
          , billTypeName = null
          , isActive = null;

        if ($('#BillTypeCode').val() == null) { billTypeCode = ""; } else { billTypeCode = $('#BillTypeCode').val(); }
        if ($('#BillTypeName').val() == null) { billTypeName = ""; } else { billTypeName = $('#BillTypeName').val(); }
        if (document.getElementById('IsActive') == null) { isActive = ""; } else { isActive = $('#IsActive').combo('getValue'); }

        window.location.href = "/StockInBillType/CreateExcelToClient/"
                                + "?billTypeCode=" + billTypeCode
                                + "&billTypeName=" + billTypeName
                                + "&isActive=" + isActive;
    }
</script>
