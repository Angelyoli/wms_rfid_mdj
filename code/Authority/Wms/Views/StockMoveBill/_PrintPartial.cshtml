<script type="text/javascript">
    function printClick() {
        var rowBill = '';
        var rows = $('#details').datagrid('getSelections');
        for (var i = 0; i < rows.length; i++) {
            rowBill += rows[i].BillNo + ",";
        }
        if (rowBill) {

            $.messager.confirm('打印', '是否打印包含异型烟', function (r) {
                var isAbnormity = false;
                if (r) {
                    isAbnormity = true;
                }
                var isGroup = true;
                window.location.href = '/StockMoveBill/CreateExcelToClient/?billNo=' + rowBill + '&isAbnormity=' + isAbnormity + '&isGroup=' + isGroup;
            });
        } else {
            $.messager.alert('系统提示', '未选中行', "info");
        }
    }
</script>
