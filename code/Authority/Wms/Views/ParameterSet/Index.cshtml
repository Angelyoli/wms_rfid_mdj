        fitcolumns="true" rownumbers="true" toolbar="#tool">
        <thead>
            <tr>
                <th field="Id" width="80" hidden="true">
                    ID
                </th>
                <th field="ParameterName" width="80">
                    参数名称
                </th>
                <th field="ParameterValue" width="80">
                    参数值
                </th>
                <th field="Remark" width="80">
                    备注
                </th>
                <th field="UserName" width="80">
                    用户名
                </th>
                <th field="SystemID" width="80">
                    系统ID
                </th>
            </tr>
        </thead>
    </table>
</div>
