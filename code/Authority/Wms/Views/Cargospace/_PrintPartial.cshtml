<script type="text/javascript">
    function printClick() {
        var row = $('#warehouseTree').tree("getSelected");
        var treeObj = $.fn.zTree.getZTreeObj("warehouseTree");
        var row1 = treeObj.getSelectedNodes();
        if (row1 == null) {
            $.messager.alert("系统提示", "未选中树形菜单", "info");
        }
        else {
            var rowAttribute = null
              , rowId = null;

            if (row1[0].type == null) { rowAttribute = ""; } else { rowAttribute = row1[0].type; }
            if (row1[0].id == null) { rowId = ""; } else { rowId = row1[0].id; }

            window.location.href = "/Cargospace/CreateExcelToClient/"
                                    + "?type=" + rowAttribute
                                    + "&id=" + rowId;
        }
    }
</script>
