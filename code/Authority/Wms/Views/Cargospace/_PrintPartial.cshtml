<script type="text/javascript">
    function printClick() {
        var row = $('#warehouseTree').tree("getSelected");
        if (row) {
            var rowAttribute = null
            var rowId = null;

            if (row.attributes == null) {
                rowAttribute = ""; 
            }
            else {
                rowAttribute = row.attributes; 
            }
            if (row.id == null) {
                rowId = ""; 
            }
            else {
                rowId = row.id; 
            }

            window.location.href = "/Cargospace/CreateExcelToClient/"
                                    + "?type=" + rowAttribute
                                    + "&id=" + rowId;
        }
        else {
            $.messager.alert("系统提示","未选中树形菜单","info");
        }
    }
</script>
